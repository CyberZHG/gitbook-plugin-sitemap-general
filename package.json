{
    "name": "gitbook-plugin-sitemap",
    "description": "Generate a sitemap for a GitBook",
    "main": "index.js",
    "version": "1.0.1",
    "engines": {
        "gitbook": ">=2.0.0"
    },
    "homepage": "https://github.com/GitbookIO/plugin-sitemap",
    "keywords": ["seo"],
    "repository": {
        "type": "git",
        "url": "https://github.com/GitbookIO/plugin-sitemap.git"
    },
    "dependencies": {
        "sitemap": "1.1.0"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/GitbookIO/plugin-sitemap/issues"
    },
    "gitbook": {
        "properties": {
            "hostname": {
                "type": "string",
                "required": true,
                "title": "Hostname for the book"
            }
        }
    }
}